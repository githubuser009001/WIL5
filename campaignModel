const mongoose = require('mongoose');

const campaignSchema = new mongoose.Schema({
    campaignTitle: {
        type: String,
        required: true
    },
    campaignDescription: {
        type: String,
        required: true
    },
    targetAudience: {
        type: String,
        required: true
    },
    issue: {
        type: String,
        required: true
    },
    expectedOutcomes: {
        type: String,
        required: true 
    },
    donationGoals: {
        type: Number,
        required: true
    } 
});

const Campaign = mongoose.model('Campaign', campaignSchema);

module.exports = Campaign;
